# Ignore files and directories generated by IDEs
.idea/
.vscode/

# Ignore compiled binary files and libraries
*.exe
*.dll
*.so
*.dylib

# Ignore package files and their dependencies
node_modules/
package-lock.json
yarn.lock

# Ignore log files and backups
*.log
*.backup

# Ignore environment-specific files
.env
.env.local
.env.*.local
.env.development.local
.env.test.local
.env.production.local

# Ignore user-specific files
*.iml
*.DS_Store
Thumbs.db

# Ignore sensitive information
config/secrets.yml
